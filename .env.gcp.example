# GCP-specific environment variables

GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1
GCS_BUCKET=truelift-ml-models

# BigQuery
BIGQUERY_DATASET=truelift_data

# Vertex AI
VERTEX_ENDPOINT_ID=projects/PROJECT_ID/locations/REGION/endpoints/ENDPOINT_ID

# Confluent Cloud (from architecture diagram)
CONFLUENT_CLOUD_BOOTSTRAP_SERVERS=pkc-xxxxx.us-central1.gcp.confluent.cloud:9092
CONFLUENT_CLOUD_API_KEY=your_api_key
CONFLUENT_CLOUD_API_SECRET=your_api_secret

# Cloud SQL
CLOUD_SQL_CONNECTION_NAME=project:region:instance
CLOUD_SQL_DATABASE=truelift
CLOUD_SQL_USER=postgres
CLOUD_SQL_PASSWORD=your_password

# Cloud Run Inference Service
INFERENCE_SERVICE_URL=https://inference-service-xxxxx-uc.a.run.app
